<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>public_id</th>
      <th>email</th>
      <th>full name</th>
      <th>role</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |account| %>
      <tr>
        <td><%= account.id %></td>
        <td><%= account.id %></td>
        <td><%= account.email %></td>
        <td><%= account.full_name %></td>
        <td><%= account.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>
